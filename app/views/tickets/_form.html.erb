<%= form_with(model: ticket, local: true) do |form| %>
  <% if ticket.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(ticket.errors.count, "error") %> prohibited this ticket from being saved:</h2>

      <ul>
        <% ticket.errors.full_messages.each do |message| %>
          <li><%= message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :User_id %>
    <%= form.text_field :user_id %>
  </div>

  <div class="field">
    <%= form.label :Task_id %>
    <%= form.text_field :task_id %>
  </div>

  <div class="field">
    <%= form.label :Description %>
    <%= form.text_area :ticket_d %>
  </div>

  <div class="field">
    <%= form.label :ticket_start %>
    <%= form.datetime_select :ticket_start %>
  </div>

  <div class="field">
    <%= form.label :ticket_end %>
    <%= form.datetime_select :ticket_end %>
  </div>



  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
